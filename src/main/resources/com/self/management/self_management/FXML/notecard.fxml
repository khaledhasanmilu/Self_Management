<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane prefHeight="251.0" prefWidth="236.0" style="-fx-background-color: white; -fx-background-radius: 20px; -fx-border-radius: 20px;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.self.management.self_management.Controller.NoteCard">
   <children>
      <TextFlow layoutY="-2.0" prefHeight="67.0" prefWidth="250.0" textAlignment="CENTER">
         <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="30.0" />
         </padding>
         <children>
            <Label fx:id="noteTitle" alignment="CENTER" contentDisplay="CENTER" text="Label">
               <font>
                  <Font name="Copperplate Gothic Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </TextFlow>
      <Label fx:id="date" layoutX="101.0" layoutY="228.0" text="Last Edit :" />
      <TextFlow layoutX="5.0" layoutY="68.0" prefHeight="155.0" prefWidth="243.0">
         <children>
            <Text fx:id="noteBody" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </TextFlow>
   </children>
   <effect>
      <DropShadow>
         <color>
            <Color opacity="0.5" />
         </color>
      </DropShadow>
   </effect>
</AnchorPane>
